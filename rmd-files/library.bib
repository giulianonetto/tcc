keywords = {Albumins,Animals,Blood Gas Analysis,Body Fluid Compartments,Bronchoalveolar Lavage Fluid,Capillary Permeability,Iodine Radioisotopes,Leukocyte Count,Lung Compliance,Lysophosphatidylcholines,Macrophages: Alveolar,Male,Oleic Acid,Organ Size,Pneumonia,Pulmonary Alveoli,Rats,Rats: Inbred Strains,Respiratory Distress Syndrome: Adult,Surface-Active Agents},
number = {6},
pages = {L1091--102},
pmid = {11076799},
title = {{Lung function, permeability, and surfactant composition in oleic acid-induced acute lung injury in rats}},
url = {papers://0ec66b90-69fd-4d38-a4ce-d1d05bec8076/Paper/p1908},
volume = {279},
year = {2000}
}
