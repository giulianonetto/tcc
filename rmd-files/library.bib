pages = {1--14},
pmid = {26358827},
publisher = {Nature Publishing Group},
title = {{Non-Classical monocytes display inflammatory features: Validation in Sepsis and Systemic Lupus Erythematous}},
url = {http://dx.doi.org/10.1038/srep13886},
