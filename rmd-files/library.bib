@article{Mizumura2016,
abstract = {By regulating the cell number and eliminating harmful cells, programmed cell death plays a critical role in development, homeostasis, and disease. While apoptosis is a recognized form of programmed cell death, necrosis was considered a type of uncontrolled cell death induced by extreme physical or chemical stress. However, recent studies have revealed the existence of a genetically programmed and regulated form of necrosis, termed necroptosis. Necroptosis is defined as necrotic cell death that is dependent on receptor-interacting protein kinase 3 (RIPK3). RIPK3, receptor-interacting protein kinase 1 (RIPK1), and a mixed-lineage kinase domain-like protein (MLKL) form a multiprotein complex called a necrosome. Although necroptosis generally provides a cell-autonomous host defense, on the other hand, cell rupture caused by necroptosis induces inflammation through the release of damage-associated molecular patterns, such as mitochondrial DNA, HMGB1, and IL-1. Previously, necroptosis was considered an alternative to apoptosis, but it is becoming increasingly clear that necroptosis itself is relevant to clinical disease, independent of apoptosis. According to some recent studies, autophagy, a cellular process for organelle and protein turnover, regulates necroptosis. This review outlines the principal components of necroptosis and provides an overview of the emerging importance of necroptosis in the pathogenesis of pulmonary disease, including chronic obstructive pulmonary disease, lung cancer, infection, and sepsis. We also discuss the molecular relationship between necroptosis and autophagy. Strategies targeting necroptosis may yield novel therapies for pulmonary diseases.},
author = {Mizumura, Kenji and Maruoka, Shuichiro and Gon, Yasuhiro and Choi, Augustine M.K. and Hashimoto, Shu},
doi = {10.1016/j.resinv.2016.03.008},
file = {:home/giulianonetto/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Yamaguchi et al. - 2017 - Protective effects of safflor yellow on acute lung injury induced by oleic acid in rats(9).pdf:pdf},
isbn = {1414-431X (Electronic)$\backslash$r0100-879X (Linking)},
issn = {22125353},
journal = {Respiratory Investigation},
keywords = {Autophagy,Chronic obstructive pulmonary disease,Mitophagy,Necroptosis},
number = {6},
pages = {407--412},
pmid = {25714887},
publisher = {Elsevier},
title = {{The role of necroptosis in pulmonary diseases}},
url = {http://dx.doi.org/10.1016/j.resinv.2016.03.008},
volume = {54},
year = {2016}
}
