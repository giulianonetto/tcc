isbn = {1535-4970 (Electronic)$\backslash$r1073-449X (Linking)},
issn = {15354970},
journal = {American Journal of Respiratory and Critical Care Medicine},
keywords = {Exosome,Lung,RNA,Rejection,Transplantation},
number = {12},
