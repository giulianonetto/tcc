issn = {18734596},
journal = {Free Radical Biology and Medicine},
keywords = {Differentiation,Growth factors,Inflammation,Macrophages,Nitro-fatty acids,Nitro-oleic acid,Signaling pathways},
number = {December 2016},
pages = {10--19},
publisher = {Elsevier B.V.},
title = {{Nitro-oleic acid regulates growth factor-induced differentiation of bone marrow-derived macrophages}},
