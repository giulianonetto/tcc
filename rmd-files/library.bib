doi = {10.1016/S0140-6736(11)60052-4},
file = {:home/giulianonetto/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/King, Pardo, Selman - 2011 - Idiopathic pulmonary fibrosis.pdf:pdf},
