doi = {10.1165/rcmb.2013-0310OC},
file = {:home/giulianonetto/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Bauer et al. - 2015 - A novel genomic signature with translational significance for human idiopathic pulmonary fibrosis.pdf:pdf},
isbn = {1044-1549},
issn = {15354989},
journal = {American Journal of Respiratory Cell and Molecular Biology},
keywords = {Animal model,Biomarkers,Genomics,Lung fibrosis},
