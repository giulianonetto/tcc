journal = {Bioscience Reports},
keywords = {-1,10,1042,2016,36,bioscience reports,bsr20160107,cite this article as,doi,e00360,fibrosis,inflammasome,inhibitors of metalloproteinase,interleukin-1,liver,lung,matrix metalloproteinase,purinergic receptors,timp,tissue},
number = {4},
