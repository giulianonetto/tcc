isbn = {1070-5287},
issn = {1531-6971},
journal = {Current opinion in pulmonary medicine},
keywords = {genomics,mmp-7,muc5b,pcmi,personalized medicine},
number = {5},
pages = {441--6},
pmid = {22847105},
title = {{Biomarkers in idiopathic pulmonary fibrosis.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22847105{\%}5Cnhttp://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4165635},
volume = {18},
