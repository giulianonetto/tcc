file = {:home/giulianonetto/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Lichtenstein et al. - 2009 - A-lines and B-lines Lung ultrasound as a bedside tool for predicting pulmonary artery occlusion pressure in.pdf:pdf},
isbn = {1931-3543 (Electronic)},
issn = {19313543},
journal = {Chest},
number = {4},
pages = {1014--1020},
pmid = {19809049},
title = {{A-lines and B-lines: Lung ultrasound as a bedside tool for predicting pulmonary artery occlusion pressure in the critically ill}},
