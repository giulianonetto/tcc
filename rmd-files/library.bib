@article{Yao-bin2013,
abstract = {Background An inflammatory response leading to organ dysfunction and$\backslash$nfailure continues to be a major problem after injury in many clinical$\backslash$nconditions such as sepsis, severe burns, and trauma. It is increasingly$\backslash$nrecognized that atrial natriuretic peptide (ANP) possesses a broad range$\backslash$nof biological activities, including effects on endothelial function and$\backslash$ninflammation. A recent study has revealed that ANP exerts$\backslash$nanti-inflammatory effects. In this study we tested the effects of human$\backslash$nANP (hANP) on lung injury in a model of oleic acid (OA)-induced acute$\backslash$nlung injury (ALI) in rats.$\backslash$nMethods Rats were randomly assigned to three groups (n=6 in each group).$\backslash$nRats in the control group received a 0.9{\%} solution of NaCl (1$\backslash$nml.kg(-1).h(-1)) by continuous intravenous infusion, after 30 minutes a$\backslash$n0.9{\%} solution of NaCl (1 ml/kg) was injected intravenously, and then$\backslash$nthe 0.9{\%} NaCl infusion was restarted. Rats in the ALI group received a$\backslash$n0.9{\%} NaCl solution (1 ml-kg(-1).h(-1)) intravenous infusion, after 30$\backslash$nminutes OA was injected intravenously (0.1 ml/kg), and then the 0.9{\%}$\backslash$nNaCl infusion was restarted. Rats in the hANP-treated ALI group received$\backslash$na hANP (0.1 mu g.kg(-1).min(-1)) infusion, after 30 minutes OA was$\backslash$ninjected intravenously (0.1 ml/kg), and then the hANP infusion was$\backslash$nrestarted. The anti-inflammation effects of hANP were evaluated by$\backslash$nhistological examination and determination of serum cytokine levels.$\backslash$nResults Serum interleukin (IL)-1 beta, IL-6, IL-10 and tumor necrosis$\backslash$nfactor (TNF) alpha were increased in the ALI group at six hours. The$\backslash$nlevels of all factors were significantly lower in the hANP treated rats$\backslash$n(P{\textless}0.005). Similarly, levels of IL-1 beta, IL-6, IL-10 and TNF-alpha$\backslash$nwere higher in the lung tissue in the ALI group at six hours. hANP$\backslash$ntreatment significantly reduced the levels of these factors in the lungs$\backslash$n(P{\textless}0.005). Histological examination revealed marked reduction in$\backslash$ninterstitial congestion, edema, and inflammation.$\backslash$nConclusion hANP can attenuate inflammation in an OA-induced lung injury$\backslash$nin rat model.},
author = {Yao-bin, Zhu and Yan-bo, Zhang and Dong-hai, Liu and Xiao-feng, Li and Ai-jun, Liu and Xiang-ming, Fan and Chen-hui, Qiao and Feng, Ling and Ying-long, Liu},
