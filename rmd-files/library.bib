@article{Mildner2016,
abstract = {Monocytes are short-lived mononuclear phagocytes that circulate in the bloodstream and comprise two main subpopulations that in the mouse are best defined by the Ly6C marker. Intravascular functions of " classical " Ly6C + monocytes and their interactions with other lymphoid and myeloid leukocytes in the circulation remain poorly understood. Rather, these cells are known to efficiently extravasate into tissues. Indeed, Ly6C + monocytes and their descendants have emerged as a third, highly plastic and dynamic cellular system that complements the two classical, tissue-resident mononuclear phagocyte compartments, i.e., macrophages and dendritic cells, on demand. Following recruitment to injured tissue, Ly6C + monocytes respond to local cues and can critically contribute to the initiation and resolution of inflammatory reactions. The second main murine monocyte subset, Ly6C – cells, derive in steady state from Ly6C + monocytes and remain in the vasculature, where the cells act as scavengers. Moreover, a major fraction of Ly6C – monocytes adheres to the capillary endothelium and patrols the vessel wall for surveillance. Given the central role of monocytes in homeostasis and pathology, in-depth study of this cellular compartment can be highly informative on the health state of the organism and provides an attractive target for therapeutic intervention.},
author = {Mildner, Alexander and Marinkovic, Goran and Jung, Steffen},
doi = {10.1128/microbiolspec.MCHD-0033-2016},
file = {:home/giulianonetto/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Mildner, Marinkovic, Jung - 2016 - Murine Monocytes Origins, Subsets, Fates, and Functions.pdf:pdf},
issn = {2165-0497},
journal = {Microbiology Spectrum},
number = {5},
pages = {1--11},
pmid = {27780020},
title = {{Murine Monocytes: Origins, Subsets, Fates, and Functions}},
url = {http://www.asmscience.org/content/journal/microbiolspec/10.1128/microbiolspec.MCHD-0033-2016},
volume = {4},
year = {2016}
}
@article{Venosa2016,
abstract = {Nitrogen mustard (NM) is an alkylating agent known to cause extensive pulmonary injury progressing to fibrosis. This is accompanied by a persistent macrophage inflammatory response. In these studies we characterized the phenotype of macrophages accumulating in the lung over time following NM exposure. Treatment of rats with NM (0.125 mg/kg, i.t.) resulted in an increase in CD11b+ macrophages in histologic sections. These cells consisted of iNOS+ proinflammatory M1 macrophages, and CD68+, CD163+, CD206+, YM-1+ and ARG-II+ antiinflammatory M2 macrophages. Whereas M1 macrophages were prominent 1-3 d post NM, M2 macrophages were most notable at 28 d. At this time, they were enlarged and vacuolated, consistent with a profibrotic phenotype. Flow cytometric analysis of isolated lung macrophages identified three phenotypically distinct subpopulations: mature CD11b-CD43- and CD68+ resident macrophages, which decreased in numbers after NM, and two infiltrating (CD11b+) macrophage subsets: immature CD43+ M1 macrophages and mature CD43- M2 macrophages, which sequentially increased. Time related increases in M1 (iNOS, IL-12alpha, COX-2, TNFalpha, MMP-9, MMP-10) and M2 (IL-10, PTX-2, CTGF, ApoE) genes, as well as chemokines/chemokine receptors associated with trafficking of M1 (CCR2, CCR5, CCL2, CCL5) and M2 (CX3CR1, fractalkine) macrophages to sites of injury, were also noted in macrophages isolated from the lung after NM. The appearance of M1 and M2 macrophages in the lung correlated with NM-induced acute lung injury and the development of fibrosis suggesting a potential role of these macrophage subpopulations in the pathogenic response to NM.},
author = {Venosa, Alessandro and Malaviya, Rama and Choi, Hyejeong and Gow, Andrew J. and Laskin, Jeffrey D. and Laskin, Debra L.},
doi = {10.1165/rcmb.2015-0120OC},
file = {:home/giulianonetto/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Venosa et al. - 2016 - Characterization of distinct macrophage subpopulations during nitrogen mustard-induced lung injury and fibrosis.pdf:pdf},
issn = {15354989},
journal = {American Journal of Respiratory Cell and Molecular Biology},
keywords = {Fibrosis,Inflammation,Macrophages,Nitrogen mustard},
number = {3},
pages = {436--446},
pmid = {26273949},
title = {{Characterization of distinct macrophage subpopulations during nitrogen mustard-induced lung injury and fibrosis}},
volume = {54},
year = {2016}
}
