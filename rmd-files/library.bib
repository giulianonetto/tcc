file = {:home/giulianonetto/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/O'Dwyer et al. - 2017 - Erratum The peripheral blood proteome signature of idiopathic pulmonary fibrosis is distinct from normal and i.pdf:pdf},
issn = {2045-2322},
journal = {Scientific Reports},
number = {April},
pages = {46860},
pmid = {28440314},
publisher = {Nature Publishing Group},
title = {{Erratum: The peripheral blood proteome signature of idiopathic pulmonary fibrosis is distinct from normal and is associated with novel immunological processes}},
url = {http://www.nature.com/articles/srep46860},
volume = {7},
year = {2017}
}
